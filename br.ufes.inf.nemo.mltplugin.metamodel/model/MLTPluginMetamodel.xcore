package br.ufes.inf.nemo.mltplugin.metamodel

class ModelElement {
	String name
	Object sourceElementFromVP
}

class ClassElement extends ModelElement {
	int order = "0"
	String[] stereotypes
	refers GeneralizationElement[] superTypeOf opposite superType
	refers GeneralizationElement[] subTypeOf opposite subType
	refers AssociationElement[] targetOf opposite target
	refers AssociationElement[] sourceOf opposite source
}

class GeneralizationElement extends ModelElement{
	refers ClassElement superType opposite superTypeOf
	refers ClassElement subType opposite subTypeOf 
}

class GeneralizationSetElement extends ModelElement {
	refers GeneralizationElement[] generalizations
	refers ClassElement characterizer
	boolean isDisjoint
	boolean isComplete
}

class AssociationElement extends ModelElement {
	String[] stereotypes
	refers ClassElement target opposite targetOf
	int targetLowerBound
	int targetUpperBound
	refers ClassElement source opposite sourceOf
	int sourceLowerBound
	int sourceUpperBound
}